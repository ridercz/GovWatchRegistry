<!DOCTYPE html>
<html lang="@System.Globalization.CultureInfo.CurrentUICulture.Name" data-culture="@System.Globalization.CultureInfo.CurrentCulture.Name">
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@(string.IsNullOrEmpty(this.ViewBag.Title) ? "Registr státních webů" : this.ViewBag.Title + " | Registr státních webů")</title>
    <link rel="stylesheet" type="text/css" href="https://cdnjs.cloudflare.com/ajax/libs/normalize/7.0.0/normalize.min.css" integrity="sha256-HxaKz5E/eBbvhGMNwhWRPrAR9i/lG1JeT4mD6hCQ7s4=" crossorigin="anonymous" />
    <link rel="stylesheet" type="text/css" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css" integrity="sha384-wvfXpqpZZVQGK6TAh5PVlGOfQNHSoD2xbE+QkPxCAFlNEevoEH3Sl0sibVcOQVnN" crossorigin="anonymous">
    <environment include="Development">
        <link rel="stylesheet" type="text/css" href="~/Content/Styles/site.css" />
    </environment>
    <environment exclude="Development">
        <link rel="stylesheet" type="text/css" href="~/Content/Styles/site.min.css" asp-append-version="true" />
    </environment>
    @RenderSection("Head", required: false)
</head>
<body>
    <header>
        <h1>Registr státních webů</h1>
    </header>
    <main>
        <div>
            @RenderBody()
        </div>
    </main>
    <footer>
        Copyright &copy; <a href="https://www.altairis.cz/">Altairis</a>, @DateTime.Today.Year - built using ASP.NET Core
    </footer>
    <script src="~/Content/Scripts/validation.min.js" asp-append-version="true"></script>
    <script>
        var v = new aspnetValidation.ValidationService();
        v.bootstrap();
    </script>
    @RenderSection("Scripts", required: false)
</body>
</html>
